{
  "interfaces": {
    "mac-modifier": 3,
    "network": {
            "interface": "eth2",
            "address": "10.0.0.1/24",
            "gateway": "10.0.0.2"
    },
    "access": [
      {
        "__comment__": "DHCP Client 1",
        "interface": "eth1",
        "type": "ipoe",
        "ipv6": false,
        "vlan-mode": "1:1",
        "outer-vlan-min": 1003,
        "outer-vlan-max": 1003,
        "outer-vlan-step": 1,
        "inner-vlan-min": 1,
        "inner-vlan-max": 1000,
	"stream-group-id": 1
        }
    ]
  },
  "dhcp": {
    "enable": true,
    "broadcast": false
  },
  "access-line": {
    "agent-remote-id": "7750bng-sub.{session-global}",
